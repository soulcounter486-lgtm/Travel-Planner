{
  "version": 2,
  "rewrites": [
    {
      "source": "/(api|auth)/:path*",
      "destination": "server/index.ts"
    },
    {
      "source": "/assets/:path*",
      "destination": "/dist/public/assets/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "server/index.ts"
    }
  ]
}
